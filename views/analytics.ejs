<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <title>RingCentral High Volume SMS</title>
    <link href="./img/favicon.png" type="image/vnd.microsoft.icon" rel="shortcut icon">
    <link href="./img/favicon.png" type="image/vnd.microsoft.icon" rel="icon">
    <script src="js/common.js" type="text/javascript"></script>
    <script src="js/analytics.js" type="text/javascript"></script>
    <script src="js/loadforms.js" type="text/javascript"></script>
    <script src="js/wordcloud2.js" type="text/javascript"></script>

    <script src="js/jquery-3.1.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/css/bootstrap-select.min.css">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/js/bootstrap-select.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/js/bootstrap-dialog.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/css/bootstrap-dialog.min.css">
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <link rel="stylesheet" type="text/css" href="css/ui.css">
    <!--link rel="stylesheet" type="text/css" href="css/conversation.css"-->
    <link rel="stylesheet" type="text/css" href="css/main.css">

    <!-- need this to support FireFox -->
    <!--
    <link  href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
  	<script src="http://code.jquery.com/jquery-1.9.0.js"></script>
  	<script src="http://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    -->
    <script>
      window.phoneNumbers = '<%- JSON.stringify(phoneNumbers) %>'
      $(document).ready(function(){
          $('[data-toggle="popover"]').popover();
      });
    </script>

</head>
<body onload="init()">
  <%- include('_feedback_form') %>
  <%- include('_menu_header') %>
  <section id='content'>
    <div class="row col-lg-12">
      <% if (phoneNumbers.length > 0) { %>
      <%- include('_menu') %>
      <% } else { %>
        <div id="menu-pane" class="col-xs-1 menu-pane">
          <ul class="nav">
            <!--
            <li class="menu-item disabled" id="campaign-new" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Campaigns"><a href="#"><img src="img/campaign.png" height="30"/></a></li>
            <li class="menu-item disabled" id="conversations" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Conversations"><a href="#"><img src="img/conversations.png" height="30"/></a></li>
            <li class="menu-item disabled" id="campaign-log" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Logs"><a href="#"><img src="img/campaign-log.png" height="30"/></a></li>
            <li class="menu-item disabled" id="settings" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Settings"><a href="#"><img src="img/settings.png" height="30"/></a></li>
            <li class="menu-item disabled" id="help" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Help"><a href="#"><img src="img/help.png" height="30"/></a></li>
            -->
            <li class="menu-item active" id="analytics" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Analytics"><a href="/analytics"><img src="img/analytics.png" height="30"/></a></li>
            <li class="menu-item" id="logout" data-placement="right" data-toggle="popover" data-trigger="hover" data-content="Logout"><a href="javascript:logout()"><img src="img/logout.png" height="30"/></a></li>
          </ul>
        </div>
      <% } %>
      <div id="control-list-col" class="col-lg-2" style="width:306px">
        <div id="col2-header" class="form-inline read-message-input-block">
        <!--
          <label class="label-input" for="analytics-choices">Choices</label>
          <select id="analytics-choices" class="selectpicker filter-select" data-width="150" onchange="changeAnalyticsBase(this)">
            <option selected value="date-range">Date range</option>
            <option value="campaign-range">Campaigns</option>
          </select>
          <hr style="margin: 2px !important;border: 2px solid white"></hr>
        -->
          <div id="date-range" class="block_space">
            <div class="block_space">
              <label class="label-input" for="my-numbers">Date from</label>
              <input type="text" id="fromdatepicker" size="10" class="form-control text-input"></input>
            </div>
            <div class="block_space">
              <label class="label-input" for="my-numbers">Date to</label>
              <input type="text" id="todatepicker" size="10" class="form-control text-input"></input>
            </div>
            <div class="block_space">
              <label class="label-input" for="my-numbers">From number</label>
              <select id="my-numbers" class="selectpicker filter-select" data-width="120" title="my numbers">
                <option selected value="all">All numbers</option>
                <% for (var item of phoneNumbers) { %>
                <option value="<%- item.number %>"><%= item.format %></option>
                <% } %>
              </select>
            </div>
            <button class="btn-rc inline" onclick="readMessageStore()">Read</button>
          </div>
        <!--
          <div id="campaign-range" class="block_space" style="display: none;">
            <select id="my-campaigns" class="selectpicker filter-select" multiple data-width="150" title="my campaigns">
            </select>
            <button class="btn-rc inline" onclick="readMessageStoreCampaign()">Read</button>
          </div>
        -->
        </div>
        <hr style="margin: 2px !important;border: 2px solid white"></hr>
        <div id="processing" style='display:none'>
          <img src='./img/processing-indicator.gif' style='width:280px;height:200px;display: block;margin:auto;'></img>
          <div style='width:100%; margin-left: 100px;font-size: 1.2em'>Processing ...</div>
        </div>
      </div>
      <div id="message-col" class="col-lg-9" style="background-color: #f5f6fb;">
        <div id='display-mode' class="row col-xs-12">
          <div class="col-lg-2 display-mode-label" onclick="switchDisplayMode()">
            <img id='mode-icon' class="display-mode-btn" src='./img/table.png'></img>
            <span id='mode-label'> Table view</span>
          </div>
          <span id="downloads" style="display: none;">
              <a href="#" onclick="downloadAnalytics();return false;">Download Analytics Data(CSV)</a><br>
          </span>
        </div>
        <div class="row col-xs-12">
          <div class="col-lg-10" id="total-graphs">
            <div id="total-title" class="breakout"></div>
            <div class="row col-lg-12" style='min-height: 200px;'>
              <div class="col-lg-4" id="total-by-direction"></div>
              <div class="col-lg-4" id="total-by-cost"></div>
              <div class="col-lg-4" id="total-by-status"></div>
            </div>

            <div id='options-bar' class="row col-lg-12" style='margin-top: 12px;display: none;'>
              <label class="label-input" for="my-numbers">Breakout option</label>
              <select id="display" class="selectpicker filter-select" data-width="200" title="Display mode" onchange="displayAnalytics()">
                <option selected value="monthly">Per Month</option>
                <option value="bynumber">Per Service Number</option>
                <!--
                <option value="hourly">Round the Clock</option>
                <option value="weekdays">Week days</option>
                -->
                <option value="failure-analytics">Failure Analytics</option>
              </select>
              <span id="sub-category">
                <label for="analytics-type">by</label>
                <select id="analytics-type" class="selectpicker filter-select" data-width="220" title="Display mode" onchange="displayAnalyticsType()">
                  <option selected value="message-count">Direction</option>
                  <option value="message-status">Outbound Message Status</option>
                  <option value="message-cost">Cost</option>
                </select>
              </span>
              <span id="failure-category" style="display:none">
                <label for="my-numbers">by</label>
                <select id="failure-types" class="selectpicker filter-select" data-width="220" title="Display mode" onchange="displayFailureAnalyticsDetails()">
                  <option selected value="spam">Spam message</option>
                  <option value="invalid-number">Invalid number</option>
                  <option value="rejected-number">Rejected number</option>
                  <option value="optout-number">Opted-out violation</option>
                  <option value="rejected-content">Invalid message</option>
                  <option value="blocked-number">Blocked number</option>
                  <option value="other-reason">Other reasons</option>
                </select>
              </span>
            </div>
            <div class="row col-lg-12 block_space">
              <div class="col-lg-6" id="total-error-breakdowns"></div>
            </div>
          </div>
          <div class="col-lg-10" id="graphs">
            <div id="statistics-title" class="breakout"></div>
            <div id="statistics"></div>
            <div id="analysis-title" class="breakout"></div>
            <div id="analysis"></div>
          </div>
          <div class="col-lg-10" id="failure-analytics" style="display:none">
            <div class='col-lg-4' id='graph-column'></div>
            <div class='col-lg-8'>
              <div id='text-column' class=''>
                <!--canvas id="my_canvas" class="canvas_container"></canvas-->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%- include('_footer') %>
</body>
</html>
