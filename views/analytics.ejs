<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <title>RingCentral High Volume SMS</title>
    <link href="./img/favicon.ico" type="image/vnd.microsoft.icon" rel="shortcut icon">
    <link href="./img/favicon.ico" type="image/vnd.microsoft.icon" rel="icon">
    <script src="js/common.js" type="text/javascript"></script>
    <script src="js/analytics.js" type="text/javascript"></script>
    <script src="js/loadforms.js" type="text/javascript"></script>

    <script src="js/jquery-3.1.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/css/bootstrap-select.min.css">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.13.3/js/bootstrap-select.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/js/bootstrap-dialog.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/css/bootstrap-dialog.min.css">
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

    <link rel="stylesheet" type="text/css" href="css/ui.css">
    <!--link rel="stylesheet" type="text/css" href="css/conversation.css"-->
    <link rel="stylesheet" type="text/css" href="css/main.css">

    <!-- need this to support FireFox -->
    <!--
    <link  href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
  	<script src="http://code.jquery.com/jquery-1.9.0.js"></script>
  	<script src="http://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    -->
    <script>
      window.phoneNumbers = '<%- JSON.stringify(phoneNumbers) %>'
      $(document).ready(function(){
          $('[data-toggle="popover"]').popover();
      });
    </script>

</head>
<body onload="init()">
  <%- include('_feedback_form') %>
  <%- include('_menu_header') %>
  <section id='content'>
    <div class="row col-lg-12">
      <%- include('_menu') %>
      <div id="control-list-col" class="col-lg-2" style="width:340px">
        <div id="col2-header" class="form-inline read-message-input-block">
          <div class="block_space">
            <label class="label-input" for="my-numbers">Date from</label>
            <input type="text" id="fromdatepicker" size="10" class="form-control text-input"></input>
          </div>
          <div class="block_space">
            <label class="label-input" for="my-numbers">Date to</label>
            <input type="text" id="todatepicker" size="10" class="form-control text-input"></input>
          </div>
          <div class="block_space">
            <label class="label-input" for="my-numbers">From number</label>
            <select id="my-numbers" class="selectpicker filter-select" data-width="150" title="my numbers">
              <option selected value="all">All numbers</option>
              <% for (var item of phoneNumbers) { %>
              <option value="<%- item.number %>"><%= item.format %></option>
              <% } %>
            </select>
          </div>
          <button class="btn-rc inline" onclick="readMessageStore()">Read</button>
        </div>
        <hr style="margin: 2px !important;border: 2px solid white"></hr>
        <div class="block_space">
          <label class="label-input" for="my-numbers">Display mode</label>
          <select id="display-mode" class="selectpicker filter-select" data-width="150" title="Display mode" onchange="displayAnalytics()">
            <option selected value="graphics">Graphics</option>
            <option value="table">Table</option>
          </select>
        </div>
        <div class="block_space">
          <label class="label-input" for="my-numbers">Display option</label>
          <select id="display" class="selectpicker filter-select" data-width="150" title="Display mode" onchange="displayAnalytics()">
            <option selected value="monthly">Monthly</option>
            <option value="hourly">Hourly</option>
            <option value="weekdays">Week days</option>
            <option value="bynumber">By Service Number</option>
            <option value="bysegment">By Message Segment</option>
            <option value="delivery_failure">Delivery failure</option>
          </select>
        </div>
        <div class="block_space">
          <label class="label-input" for="my-numbers">Error code</label>
          <select id="errors" class="selectpicker filter-select" data-width="150" title="Display mode" onchange="displayDeliveryFailedAnalytics()">
            <option selected value="SMS-CAR-430">Spam</option>
            <option value="SMS-CAR-104">No Receipt</option>
            <option value="SMS-CAR-411">Destination</option>
          </select>
        </div>
      </div>
      <div id="message-col" class="col-lg-8" style="background-color: #f5f6fb;">
        <div class="row col-xs-12">
          <div class="col-lg-10" id="total-graphs">
            <div class="row col-lg-12">
              <div class="col-lg-4" id="total-by-direction"></div>
              <div class="col-lg-4" id="total-by-cost"></div>
              <div class="col-lg-4" id="total-by-status"></div>
            </div>
            <div class="row col-lg-12 block_space">

              <div class="col-lg-6" id="total-error-breakdowns"></div>
            </div>
          </div>
          <div class="col-lg-10" id="graphs">
            <div id="by_direction"></div>
            <div class="block_space"></div>
            <div id="by_cost"></div>
            <div class="block_space"></div>
            <div id="by_status"></div>
          </div>
          <div class="col-lg-2">
            <span class="block_space" id="downloads" style="display: none">
              <a href="#" onclick="downloadMessageStore('CSV');return false;" style="display: inline">Download (CSV)</a><br>
              <a href="#" onclick="downloadMessageStore('JSON');return false;" style="display: inline">Download (JSON)</a>
            </span>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%- include('_footer') %>
</body>
</html>
